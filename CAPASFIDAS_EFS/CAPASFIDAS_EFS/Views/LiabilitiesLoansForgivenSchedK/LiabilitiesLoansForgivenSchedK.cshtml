                    sessionStorage.setItem("ExistingLiabilityAutoComp", "Yes");

                    $.each(results, function (i, item) {
                        $("#txtPayeeNameShedF").val(item.FilingEntityName.toString());
                        $("#txtStreetNameShedF").val(item.FilingEntityStreetName.toString());
                        $("#txtCityShedF").val(item.FilingEntityCity.toString());
                        $("#txtStateShedF").val(item.FilingEntityState.toString());
                        $("#txtZipCodeShedF").val(item.FilingEntityZip.toString());
                        if (item.FilingEntityCountry == null) {
                            $("#txtCountryShedF").val("");
                        }
                        else {
                            $("#txtCountryShedF").val(item.FilingEntityCountry.toString());
                        }

                        if ($("#txtCountryShedF").val().toString() == "United States") {
                            $("#chkCountryShedF").prop('checked', false);
                            $("#txtCountryShedF").prop("disabled", true);
                            $("#txtStateShedF").prop('maxLength', '2');
                            if (item.FilingEntityZip.toString() != "") {
                                $("#txtZipCodeShedF").removeClass("watermarkHTMLTextBox");
                            }
                            else {
                                $("#txtZipCodeShedF").addClass("watermarkHTMLTextBox");
                                $("#txtZipCodeShedF").blur();
                            }
                        }
                        else {
                            $("#chkCountryShedF").prop('checked', true);
                            $("#txtCountryShedF").prop("disabled", false);
                            $("#txtStateShedF").prop('maxLength', '30');

                            $m('#txtZipCodeShedF').attr("placeholder", "");
                            $("#txtZipCodeShedF").removeClass("watermarkHTMLTextBox");
                        }

                        $m("#txtPayeeNameShedF").focus();
                        $m("#txtPayeeNameShedF").valid();
                        // Address validation
                        $m("#txtCountryShedF").valid();
                        $m("#txtStreetNameShedF").valid();
                        $m("#txtCityShedF").valid();
                        $m("#txtStateShedF").valid();
                        $m("#txtZipCodeShedF").valid();

                        sessionStorage.setItem("LibFlngEntyId", item.FilingEntityId.toString());
                        sessionStorage.setItem("AutoCompltPayeeName", item.FilingEntityName.toString());

                    });

                    if ($("#lstLiabilityExists option:selected").val().toString() == "Y") {

                        // Load Date Incurred Data.
                        // Get Date Incurred Data.
                        // Bind Date Incurred Data.
                        var options = {};
                        options.url = "@Url.Content("~/ExpenditurePaymentsSchedF/GetDateIncurredForForgiven/")";
                        options.type = "POST";
                        options.data = JSON.stringify(
                            {
                                strFilingEntityId: sessionStorage.getItem("LibFlngEntyId").toString()
                            });
                        options.dataType = "json";
                        options.contentType = "application/json";
                        options.success = function (resultsDateIncurred) {
                            $("#lstDateIncurred").empty();
                            $("#lstDateIncurred").append("<option value='" + '0' + "'>" + "- Select -" + "</option>");
                            for (var i = 0; i < resultsDateIncurred.length; i++) {
                                $("#lstDateIncurred").append("<option value='" + resultsDateIncurred[i].Value + "'>" + resultsDateIncurred[i].Text + "</option>");
                            }
                        };
                        options.error = function () {
                            ShowDialogBox('EFS', 'Error Fetching Data. Please Contact NYS Board of Elections ◾(518) 474-8200 ', 'Ok', '');
                        };
                        $.ajax(options);
                    }

                });

            },
            change: function (event, ui) {
                if (ui.item) {
                    // do whatever you want to when the item is found
                    $m("label[for=txtPayeeNameShedF]").text('');
                    $m("#txtPayeeNameShedF").removeClass("error");
                }
                else {
                    // do whatever you want to when the item is not found
                    $m("label[for=txtPayeeNameShedF]").text('');
                    $m("#txtPayeeNameShedF").removeClass("error");
                }
            }
        });

        // FOCUS IN WHEN EXISTING LIABILITY
        // WE HAVE TO RESET THE DATE INCURRED AND AMOUNT.
        $("#txtPayeeNameShedF").focusin(function () {
            if ($("#lstLiabilityExists option:selected").val().toString() == "Y") {

                $("#lstDateIncurred").empty();
                $("#lstDateIncurred").append("<option value='" + '0' + "'>" + "- Select -" + "</option>");

                $("#txtAmtExpenditurePayments").val("");
                $("#txtAmtExpenditurePayments").blur();
                $("#txtOutstandingAmtShedF").val("");
                $("#txtOutstandingAmtShedF").blur();
            }
        });

        // Partial Liability and if Existing Liabilty Yes then it will bring
        // Date Incurred and Original Amount Dropdown will show.
        $("#txtPayeeNameShedF").on('blur', function (e) {

            if ($("#txtPayeeNameShedF").val() != "") {

                if ($("#lstLiabilityExists option:selected").val().toString() == "Y") {

                    // GET THE FILING ENTITY EXISTS OR NOT.
                    $.post("@Url.Content("~/ExpenditurePaymentsSchedF/GetPayeeNameExistsLiability/")", {
                        strPayeeName: $("#txtPayeeNameShedF").val().toString()
                    }, function (resultsLiability) {
                        if (resultsLiability.length != 0) {

                            $.each(resultsLiability, function (i, item) {
                                sessionStorage.setItem("EntityIdLiab", item.FilingEntityId.toString());
                            });

                            // Bind Date Incurred Data.
                            $.post("@Url.Content("~/ExpenditurePaymentsSchedF/GetDateIncurredForForgiven/")", {
                                strFilingEntityId: sessionStorage.getItem("EntityIdLiab").toString()
                            }, function (results) {

                                if (results.length != 0) {

                                    $("#lstDateIncurred").empty();
                                    $("#lstDateIncurred").append("<option value='" + '0' + "'>" + "- Select -" + "</option>");
                                    for (var i = 0; i < results.length; i++) {
                                        $("#lstDateIncurred").append("<option value='" + results[i].Value + "'>" + results[i].Text + "</option>");
                                    }
                                    $("#txtAmtExpenditurePayments").val("");
                                    $("#txtAmtExpenditurePayments").blur();
                                    $("#txtOutstandingAmtShedF").val("");
                                    $("#txtOutstandingAmtShedF").blur();
                                }
                                else {
                                    $("#lstDateIncurred").empty();
                                    $("#lstDateIncurred").append("<option value='" + '0' + "'>" + "- Select -" + "</option>");

                                    $("#txtAmtExpenditurePayments").val("");
                                    $("#txtAmtExpenditurePayments").blur();
                                    $("#txtOutstandingAmtShedF").val("");
                                    $("#txtOutstandingAmtShedF").blur();
                                    ShowDialogBox('EFS', 'Payee Name not existing in liability', 'Ok', '');
                                }
                            });
                        }
                        else {
                            $("#lstDateIncurred").empty();
                            $("#lstDateIncurred").append("<option value='" + '0' + "'>" + "- Select -" + "</option>");

                            $("#txtAmtExpenditurePayments").val("");
                            $("#txtAmtExpenditurePayments").blur();
                            $("#txtOutstandingAmtShedF").val("");
                            $("#txtOutstandingAmtShedF").blur();

                            ShowDialogBox('EFS', 'Payee Name not existing in liability', 'Ok', '');
                        }
                    });
                }
            }
        });

        $m("#txtAmtExpenditurePayments").focusout(function () { // come here

            if ($m("#lstLiabilityExists option:selected").val().toString() == 'Y') {
                if ($("#lstDateIncurred option:selected").text().toString() != "- Select -") {
                    if ($("#txtAmtExpenditurePayments").val().toString() != "") {
                        if (sessionStorage.getItem("OutStandingAmount").toString() != "") {
                            var payAmount = $("#txtAmtExpenditurePayments").val().toString();
                            var outAmount = sessionStorage.getItem("OutStandingAmount").toString();

                            outAmount = parseFloat(outAmount); //.toFixed(2);
                            payAmount = parseFloat(payAmount); //.toFixed(2);

                            if (outAmount < payAmount) {

                                $("#txtOutstandingAmtShedF").val(parseFloat(outAmount).toFixed(2).toString());

                                $("#txtAmtExpenditurePayments").val("");
                                $("#txtAmtExpenditurePayments").blur();

                                ShowDialogBox('EFS', 'Amount Forgiven should not be more then Outstanding Amount.', 'Ok', '');
                            }
                            else {
                                var outstandingAmount = parseFloat(outAmount) - parseFloat(payAmount);
                                $("#txtOutstandingAmtShedF").val(outstandingAmount.toFixed(2).toString());
                            }
                        }
                    }
                    else {
                        if ($("#lstLiabilityExists option:selected").val().toString() == "Y") {
                            $("#txtOutstandingAmtShedF").val(parseFloat(sessionStorage.getItem("OutStandingAmount")).toFixed(2).toString());
                        }
                        else {
                            $("#txtOutstandingAmtShedF").val("");
                            $("#txtOutstandingAmtShedF").blur();
                        }
                    }
                }
                else {
                    $("#txtOutstandingAmtShedF").val("");
                    $("#txtOutstandingAmtShedF").blur();
                }
            }
            else {
                if ($("#lstLiability option:selected").val().toString() == "Y") {
                    if ($("#txtAmtExpenditurePayments").val().toString() != "") {
                        if ($("#txtOriginalAmountShedF").val().toString() != "") {
                            var payAmount = $("#txtAmtExpenditurePayments").val().toString();
                            var originalAmount = $("#txtOriginalAmountShedF").val().toString();

                            payAmount = parseFloat(payAmount);
                            originalAmount = parseFloat(originalAmount);

                            if (payAmount > originalAmount) {
                                $("#txtAmtExpenditurePayments").val("");
                                $("#txtAmtExpenditurePayments").blur();

                                ShowDialogBox('EFS', 'Amount Forgiven should not be more then Original Amount.', 'Ok', '');
                            }
                            else {
                                var outstandingAmount = parseFloat(originalAmount) - parseFloat(payAmount);
                                $("#txtOutstandingAmtShedF").val(outstandingAmount.toFixed(2).toString());
                            }
                        }
                    }
                    else {
                        $("#txtOutstandingAmtShedF").val("");
                        $("#txtOutstandingAmtShedF").blur();
                    }
                }
                else {
                    $("#txtAmtExpenditurePayments").val("");
                    $("#txtOutstandingAmtShedF").val("");
                    $("#txtAmtExpenditurePayments").blur();
                    $("#txtOutstandingAmtShedF").blur();
                }
            }
        });

        $("#txtOriginalAmountShedF").focusout(function () {
            if ($m("#lstLiability option:selected").val().toString() == 'N') {
                if ($m("#lstLiabilityExists option:selected").val().toString() == 'N') {
                    if ($("#txtOriginalAmountShedF").val() != "") {
                        var orignalAmount = $("#txtOriginalAmountShedF").val().toString();
                        var outstandingAmount = parseFloat(orignalAmount) - parseFloat(orignalAmount);
                        $("#txtAmtExpenditurePayments").val(parseFloat(orignalAmount).toFixed(2).toString());
                        $("#txtOutstandingAmtShedF").val(outstandingAmount.toFixed(2).toString());
                        $("#txtAmtExpenditurePayments").prop("disabled", true);

                        $("#txtExplanationCommonShedF").focus();

                        $m("label[for=txtAmtExpenditurePayments]").text('');
                        $m("#txtAmtExpenditurePayments").removeClass("error");

                    }
                    else {
                        $("#txtAmtExpenditurePayments").val("");
                        $("#txtOutstandingAmtShedF").val("");
                        $("#txtAmtExpenditurePayments").blur();
                        $("#txtOutstandingAmtShedF").blur();

                        $("#txtAmtExpenditurePayments").prop("disabled", false);
                    }
                }
                else {
                    $("#txtAmtExpenditurePayments").prop("disabled", false);
                }
            }
            else {
                $("#txtAmtExpenditurePayments").val("");
                $("#txtOutstandingAmtShedF").val("");

                $("#txtAmtExpenditurePayments").prop("disabled", false);
            }
        });

        $m("#btnClearDialogSchedF").button().on("click", function () {
            var validator = $m("#DialogBoxSchedKFormLiabilities").validate();
            validator.resetForm();  // clear out the validation errors
            $m("#DialogBoxSchedKFormLiabilities")[0].reset();   // clear out the form data

            $("#txtCountryShedF").removeClass("ui-state-error error");
            $("#txtStreetNameShedF").removeClass("ui-state-error error");
            $("#txtCityShedF").removeClass("ui-state-error error");
            $("#txtStateShedF").removeClass("ui-state-error error");
            $("#txtZipCodeShedF").removeClass("ui-state-error error");
            $("#txtCheckShedF").removeClass("ui-state-error error");
            $("#txtOriginalAmountShedF").removeClass("ui-state-error error");
            $("#txtAmtExpenditurePayments").removeClass("ui-state-error error");
            $("#txtExplanationCommonShedF").removeClass("ui-state-error error");

            $("#lblExplanation").text("Explanation");
            $("#lblExplanationSchedF").text("Explanation");

            $("#txtCountry").val("United States");
            $("#txtCountry").prop("disabled", true);
            $("#chkCountry").prop('checked', false);

            $("#txtCountryShedF").val("United States");
            $("#txtCountryShedF").prop("disabled", true);
            $("#txtCountryShedF").prop('checked', false);

            $("#txtState").prop('maxLength', '2');
            $("#lstMethod")[0].selectedIndex = 0;
            $("#tdCheck").hide();
            $("#txtCurrentDateLoanForgiven").focus();
            $m("label[for=txtCurrentDateLoanForgiven]").text('');
            $m("#txtCurrentDateLoanForgiven").removeClass("error");
            $("#lstSearchAmount").prop("disabled", true);
            $("#lstSearchDate").prop("disabled", true);
            $("#lstLiabilityExists")[0].selectedIndex = 0;
            $("#lstLiability")[0].selectedIndex = 0;

            $("#txtAmtExpenditurePayments").prop("disabled", false);

            $("#dvDateIncurred").hide();
            $("#dvOriginalAmtTxt").show();
            $("#tdCheckSchedF").hide();
        });

        $m("#btnBackSchedF").button().on("click", function () {
            dialog_Liabilities.dialog("close");//Close current dialog before closing the previous to prevent '_focusTabbable' error in jquery-ui.
            dialog.dialog("open");
            $("#divExplanation").show();
            $("#divExplanationScheN").hide();
            $("#hAddScheI").show();
            $("#divLiabilityLoans").show();
            $("#hSearch").show();
            $("#hAddScheJ").hide();
            $("#hAddScheN").hide();
            $("#divSearchLoans").hide();
            $("#divAdd").hide();
            $("#lstSearchAmount").prop("disabled", false);
            $("#lstSearchDate").prop("disabled", false);
            $("#divOriginalName").show();
            //Button Show/Hide
            $("#btnPreviousDialogForSearch").hide();
            $("#btnNextDialogForSearch").show();
            $("#btnUpdateDialog").hide();
            $("#btnAddDialog").hide();
            $("#lstLiabilityLoans")[0].selectedIndex = 2;
        });

        // Method Change
        // ------------------------------------------------------------------------------------------------------------------------------------------
        $("#lstMethodShedF").change(function () {

            $("#txtCheckShedF").val("");

            if ($("#lstMethodShedF option:selected").val().toString() == "1") { // CHECK
                $("#tdCheckSchedF").show();
                $("#lblExplanationSchedF").text("Explanation");
            }
            else if ($("#lstMethodShedF option:selected").val().toString() == "7") { // OTHER
                // EXPLANATION MANADATORY
                $("#lblExplanationSchedF").text("* Explanation");
                $("#tdCheckSchedF").hide();
            }
            else {
                $("#lblExplanationSchedF").text("Explanation");
                $("#tdCheckSchedF").hide();
            }
        });

        // Show Loan Details Link -----END-----------------------------------------------------------------------------
    });
</script>