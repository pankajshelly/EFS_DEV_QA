            <input type="button" id="btnSort" class="btn btn-default clsSort ui-button ui-widget ui-state-default ui-corner-all" name="Command" value="Sort" title="Sort" style="font-size:12px;" />
        </div>
    </div>
    <div id="divViewableColumn" style="clear:both;">
        <div class="verticalCSSDestopTISForLables lblCSS">
            <span id = "lblColumns">Select columns to view</span>
        </div>
