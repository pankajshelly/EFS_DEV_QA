            @Html.DropDownList("ddlViewableColumn", ViewData["lstViewableColumns"] as SelectList, new { @class = "spanMobileWidth clsMultiSelect", aria_labelledby = "lblColumns", multiple = "multiple" })
        </div>
        <div class="verticalCSSDestopTISForCommonGrid DDLCSS" style="padding-bottom:2em;">
            <input type="button" id="btnSetPreferences" class="btn btn-default clsSetPreserences ui-button ui-widget ui-state-default ui-corner-all" name="Command" value="Set Preferences" title="Set Preferences" style="font-size:12px;" />
        </div>
    </div>
</div>

