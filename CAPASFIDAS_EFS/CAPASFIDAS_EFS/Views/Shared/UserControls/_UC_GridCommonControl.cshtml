            if (selectedlstTransactionType == "20") {
                $("#lblHeader,#lblHeader1,#lblHeader2").text("Public Fund Expenses");
                var urlOtherReceiptsReceivedSchedE = "@Url.Content("~/PublicFundExpensesSchedT/PublicFundExpensesSchedT/")";
