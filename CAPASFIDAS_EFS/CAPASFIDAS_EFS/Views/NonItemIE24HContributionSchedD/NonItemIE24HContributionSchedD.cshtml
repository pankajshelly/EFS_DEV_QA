            $m("#DialogBoxSchedDIEForm")[0].reset();   // clear out the form data

            $("#lblExplanation").text("* Explanation");

            $("#txtCurrentDateIE24HContr").blur();
            $("#txtZipCode").blur();
            $("#txtAmtIEWeeklyContr").blur();

            $("#txtTreasurerOccupation").focus();

